    "resume-builder-resume-feature-resume-edit": {
      "projectType": "library",
      "root": "libs/resume-builder/resume/feature-resume-edit",
      "sourceRoot": "libs/resume-builder/resume/feature-resume-edit/src",
      "prefix": "coderisland",
      "architect": {
