    "omgimgflow": {
      "projectType": "application",
      "root": "apps/omgimgflow",
      "sourceRoot": "apps/omgimgflow/src",
      "prefix": "omg",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "outputPath": "dist/apps/omgimgflow",
            "index": "apps/omgimgflow/src/index.html",
            "main": "apps/omgimgflow/src/main.ts",
            "polyfills": "apps/omgimgflow/src/polyfills.ts",
            "tsConfig": "apps/omgimgflow/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/omgimgflow/src/favicon.ico",
              "apps/omgimgflow/src/assets"
            ],
            "styles": ["apps/omgimgflow/src/styles.scss"],
            "scripts": [],
            "customWebpackConfig": {
              "path": "./tailwind-configs/omgimgflow.webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/omgimgflow/src/environments/environment.ts",
                  "with": "apps/omgimgflow/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "omgimgflow:build",
            "proxyConfig": "apps/omgimgflow/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "omgimgflow:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "omgimgflow:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/omgimgflow/src/**/*.ts",
              "apps/omgimgflow/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/omgimgflow"],
          "options": {
            "jestConfig": "apps/omgimgflow/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
