{
  "version": 1,
  "projects": {
    "resume-builder": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/resume-builder",
      "sourceRoot": "apps/resume-builder/src",
      "prefix": "resb",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "tailwind-configs/resume-builder.webpack.config.js"
            },
            "outputPath": "dist/apps/resume-builder",
            "index": "apps/resume-builder/src/index.html",
            "main": "apps/resume-builder/src/main.ts",
            "polyfills": "apps/resume-builder/src/polyfills.ts",
            "tsConfig": "apps/resume-builder/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/resume-builder/src/favicon.ico",
              "apps/resume-builder/src/assets"
            ],
            "styles": ["apps/resume-builder/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/resume-builder/src/environments/environment.ts",
                  "with": "apps/resume-builder/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "resume-builder:build",
            "proxyConfig": "apps/resume-builder/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "resume-builder:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "resume-builder:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/resume-builder/tsconfig.app.json",
              "apps/resume-builder/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/resume-builder/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/resume-builder/jest.config.js",
            "passWithNoTests": true
          },
          "outputs": ["coverage/apps/resume-builder"]
        }
      }
    },
    "resume-builder-e2e": {
      "root": "apps/resume-builder-e2e",
      "sourceRoot": "apps/resume-builder-e2e/src",
      "projectType": "application",
